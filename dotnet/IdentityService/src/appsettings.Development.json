{
  "Hirameku": {
    "Caching": {
      "CacheOptions:ConnectionString": "localhost:6379"
    },
    "Common": {
      "Service": {
        "PasswordValidatorOptions:PasswordBlacklistPath": "/etc/hirameku/password-blacklist.txt",
        "SecurityTokenOptions": {
          "Audience": "https://www.local.hirameku.dev/",
          "Issuer": "https://www.local.hirameku.dev/",
          "SecretKey": "Overridden by secrets.json"
        }
      }
    },
    "Data": {
      "DatabaseOptions:ConnectionString": "mongodb://localhost:27017/"
    },
    "Email": {
      "EmailerOptions": {
        "RejectRegistrationUrl": "https://www.local.hirameku.dev/rejectRegistration",
        "ResetPasswordUrl": "https://www.local.hirameku.dev/resetPassword",
        "Sender": "noreply@localhost",
        "SmtpCredentials": {
          "Password": null,
          "UserName": null
        },
        "SmtpPort": "2525",
        "SmtpServer": "localhost",
        "UseTls": false,
        "VerifyEmailUrl": "https://www.local.hirameku.dev/verifyEmail"
      }
    },
    "Recaptcha": {
      "RecaptchaOptions:SiteSecret": "Overridden by secrets.json"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Debug"
    }
  }
}
